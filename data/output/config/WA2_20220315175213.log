syslog destination 172.x.x.x
syslog enable
syslog function all warning
!
hostname WA2
!
username esmile password plain esmile administrator
!
!
!
!
no wireless-adapter enable
!
bridge ieee enable
!
device usb0
!
device module0
!
!
interface GigaEthernet0.0
  vlan-type tagged VLAN0 tag-id 2
  vlan-type tagged VLAN1 tag-id 2
  no ip address
  no shutdown
!
interface GigaEthernet1.0
  ip address 192.168.x.x/24
  no shutdown
!
interface Loopback0.0
  ip address 172.x.x.2/32
  no shutdown
!
interface MobileEthernet0.0
  ip address dhcp
  ip tcp adjust-mss auto
  ip napt enable
  ip napt static 192.168.x.x tcp 50022 22 priority 1
  ip napt reserve tcp 22
  ip napt reserve icmp
  mobile id IP cbm.dwh.jreast.co.jp
  auto-connect
  no shutdown
!
interface MAPE0
  no ip address
  shutdown
!
interface EtherIP0
  bridge ieee 1
  bridge ip tcp adjust-mss 1264
  ether-ip peer 172.x.x.x
  ether-ip source MobileEthernet0.0
  no shutdown
!
interface VLAN0
  no ip address
  bridge ieee 1
  no shutdown
!
interface VLAN1
  ip address 172.x.x.2/30
  no shutdown
!
!
ip route default MobileEthernet0.0
ip route 172.x.x.x/30 MobileEthernet0.0
  ip route 172.24.0.0/16 172.24.x.256/32 VLAN1
!
network-monitor monitor1
  monitor interval occurrence 3
  monitor interval restorer 3
  monitor counter occurrence 20
  monitor counter restorer 3
  monitor wait-time 1 sec
  monitor startup-delay 60
  event ip unreach-host 172.x.x.2
  action 10 reset-device module0 interval 2
!
monitor-group monitor1 enable
monitor-group monitor1 directed-response
!
no mobile expected-attachment usb0 recovery-enable
!
!
!
!
terminal timeout 10
!
telnet-server ip enable
telnet-server ip permit 10.0.0.0/8
!
ssh-server ip enable
ssh-server ip permit 10.0.0.0/8
!
sntp-client enable
ntp-server server 172.x.x.x
!
snmp ip enable
snmp ip community esmile read-only
snmp ip trap destination 172.x.x.x esmile v2
snmp ip trap interface VLAN1
snmp ip trap esmile generic-trap all
snmp ip permit 10.0.0.0/8
!
!
!
led vpn ipsec
!
!
!
!
!
!
!
!
